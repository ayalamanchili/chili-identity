<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet author="mbadiginchala" id="add-billingrate-indexes">
        <createIndex catalogName="office"
                     indexName="BillingRate_EffectiveDate_IDX"
                     schemaName="public"
                     tableName="billingrate"
                     unique="false">
            <column name="effectiveDate" type="date"/>
        </createIndex>     
        <createIndex catalogName="office"
                    indexName="BillingRate_UpdatedTS_IDX"
                    schemaName="public"
                    tableName="billingrate"
                    unique="false">
            <column name="updatedTs" type="datetime"/>
        </createIndex>           
    </changeSet>
</databaseChangeLog>
