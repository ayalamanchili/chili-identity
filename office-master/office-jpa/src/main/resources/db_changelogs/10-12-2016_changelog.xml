<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    
    <changeSet author="rvallabhaneni" id="add-payrate-and-overtimepayrate-percentage">
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="clientinformation">
            <column name="payRatePercentage" type="decimal"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="clientinformation_aud">
            <column name="payRatePercentage" type="decimal"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="clientinformation">
            <column name="overTimePayRatePercentage" type="decimal"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="clientinformation_aud">
            <column name="overTimePayRatePercentage" type="decimal"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>
