<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    
    <changeSet author="mbadiginchala" id="project-add">
        <preConditions onFail="WARN"> 
        </preConditions>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="project">
            <column name="vendor_id" type="bigint"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="project_aud">
            <column name="vendor_id" type="bigint"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="project">
            <column name="middleVendor_id" type="bigint"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="project_aud">
            <column name="middleVendor_id" type="bigint"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="project">
            <column name="purchaseOrderNo" type="varchar(255)"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="project_aud">
            <column name="purchaseOrderNo" type="varchar(255)"/>
        </addColumn>
        <addColumn catalogName="office"
                           schemaName="public"
                           tableName="project">
            <column name="subContractorWorkOrderNo" type="varchar(255)"/>
        </addColumn>
        <addColumn catalogName="office"
                       schemaName="public"
                       tableName="project_aud">
            <column name="subContractorWorkOrderNo" type="varchar(255)"/>
        </addColumn>
    <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="project"
                                 constraintName="FK_Vendor_Projects"
                                 referencedColumnNames="id"
                                 referencedTableName="vendor"/>
    <addForeignKeyConstraint baseColumnNames="middleVendor_id"
                                 baseTableName="project"
                                 constraintName="FK_MiddleVendor_Projects"
                                 referencedColumnNames="id"
                                 referencedTableName="vendor"/>
    </changeSet>
</databaseChangeLog>
