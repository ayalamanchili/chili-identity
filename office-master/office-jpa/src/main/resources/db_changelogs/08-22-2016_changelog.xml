<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    
    <changeSet author="sbandaru" id="clientinformation-add-clientFee">
        <preConditions onFail="WARN"> 
        </preConditions>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="clientinformation">
            <column name="clientFeeApplicable" type="boolean"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="clientinformation_aud">
            <column name="clientFeeApplicable" type="boolean"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="clientinformation">
            <column name="directClient" type="boolean"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="clientinformation_aud">
            <column name="directClient" type="boolean"/>
        </addColumn>        
    </changeSet>
    <changeSet author="sbandaru" id="client-update-jul">
        <preConditions onFail="WARN"> 
        </preConditions>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="client">
            <column name="directClient" type="boolean"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="client_aud">
            <column name="directClient" type="boolean"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>    