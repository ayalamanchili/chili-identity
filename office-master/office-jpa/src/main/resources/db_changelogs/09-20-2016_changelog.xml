<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet author="rmukkala" id="add-missing-index-fullscan-3">
        <createIndex catalogName="office"
                     indexName="EMP_ONB_STRT_DT"
                     schemaName="public"
                     tableName="employeeonboarding"
                     unique="false">
            <column name="startedDate" type="date"/>
        </createIndex>     
        <createIndex catalogName="office"
                     indexName="INVOICE_DT"
                     schemaName="public"
                     tableName="invoice"
                     unique="false">
            <column name="invoiceDate" type="date"/>
        </createIndex>  
        <createIndex catalogName="office"
                     indexName="PRSPCT_STRT_DT"
                     schemaName="public"
                     tableName="prospect"
                     unique="false">
            <column name="startDate" type="date"/>
        </createIndex>           
    </changeSet>
</databaseChangeLog>
