<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet author="ayalamanchili" id="add-missing-index-fullscan-1">
        <createIndex catalogName="office"
                     indexName="CUSER_ENABLED"
                     schemaName="public"
                     tableName="cuser"
                     unique="false">
            <column name="enabled" type="bit"/>
        </createIndex>     
        <createIndex catalogName="office"
                     indexName="CRP_RPT_START_DT_IDX"
                     schemaName="public"
                     tableName="corporatestatusreport"
                     unique="false">
            <column name="reportStartDate" type="date"/>
        </createIndex>  
        <createIndex catalogName="office"
                     indexName="CRP_RPT_END_DT_IDX"
                     schemaName="public"
                     tableName="corporatestatusreport"
                     unique="false">
            <column name="reportEndDate" type="date"/>
        </createIndex>           
    </changeSet>
</databaseChangeLog>
