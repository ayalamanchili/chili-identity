<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet author="mbadiginchala" id="addindexes">
        <createIndex catalogName="office"
                     indexName="CI_START_DATE_IDX"
                     schemaName="public"
                     tableName="clientinformation"
                     unique="false">
            <column name="startDate" type="date"/>
        </createIndex>     
        <createIndex catalogName="office"
                    indexName="CI_END_DATE_IDX"
                    schemaName="public"
                    tableName="clientinformation"
                    unique="false">
            <column name="endDate" type="date"/>
        </createIndex> 
        <createIndex catalogName="office"
                     indexName="CI_ITEM_IDX"
                     schemaName="public"
                     tableName="clientinformation"
                     unique="false">
            <column name="itemNumber" type="varchar(255)"/>
        </createIndex>            
    </changeSet>
</databaseChangeLog>
