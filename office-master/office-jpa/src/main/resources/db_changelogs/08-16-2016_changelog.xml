        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="client">
            <column name="msaValDate" type="date"/>
        </addColumn>
        <addColumn catalogName="office"
                   schemaName="public"
                   tableName="client_aud">
            <column name="msaValDate" type="date"/>
        </addColumn>
</changeSet>
</databaseChangeLog>
