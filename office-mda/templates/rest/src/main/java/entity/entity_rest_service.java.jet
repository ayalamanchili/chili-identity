		return dao.findById(id);
	}

	@PUT
	@Path("/create")
	//@Restrict("#{s:hasRole('admin')}")
	public void create(${$class/@name} entity) throws ServiceException{
		dao.save(entity);
	}

	@PUT
	@Path("/update")
	//@Restrict("#{s:hasRole('admin')}")
	public void update(${$class/@name} object) throws ServiceException{
		${$class/@name} entity = dao.findById(object.getId());
		mapper.map(object, entity);
		dao.save(entity);
	}

	@DELETE
	@Path("/delete/{id}")
	//@Restrict("#{s:hasRole('admin')}")
